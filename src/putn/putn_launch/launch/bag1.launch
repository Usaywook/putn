<launch>
  <arg name="bag_dir" default="/root/docker-workspace/ssd512/bags/tao" />
  <arg name="filename" default="tao3.bag" />

  <node pkg="rosbag" type="play" name="player" args="$(arg bag_dir)/$(arg filename) --topics /ouster/points /joint_states" output="screen" required = "true">
    <remap from="/joint_states"             to="/joint_states/old"/>
  </node>

</launch>